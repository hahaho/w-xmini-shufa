<jwqcapsule capsule="{{ capsule }}">
  <text class="cfff">每日一字</text>
</jwqcapsule>
<image mode="widthFix" class="w100" src="{{info.img_url}}"></image>
<!--列表 -s-->
<view class="hundredListItem">
  <view class="hliTopName c323232">
    <text class="f38">{{info.title}}</text>
    <view class="f21 c787878 mt10">
      <text>{{info.create_at}}</text>
      <text>阅读{{info.hits}}</text>
    </view>
  </view>
<!--  <view class="hliContent f28 c787878 mt20">-->
<!--    {{ info.des }}-->
<!--  </view>-->
  <import src="../../wxParse/wxParse"></import>
  <template is="wxParse" data="{{ wxParseData: desc.nodes }}"></template>
</view>
<!--列表 -e-->
<!--评论列表 -s-->
<view class="comment">
  <view class="commentTop left-pillar f24 c646464 p20">
    <text>评论 {{info.discuss}}　　　赞 {{info.star}}</text>
  </view>
  <view wx:for="{{comment}}" wx:key="index" class="commentItem bd1">
    <image class="br ciImg" mode="aspectFill" lazy-load="1" src="{{item.uid_avatar_url}}"></image>
    <view class="ciRight flex1">
      <text class="f22 c787878">{{item.uid_username}}</text>
      <text class="f25 c000">{{item.comment}}</text>
<!--      <view class="p20 ciReply mt20 f22 c646464">-->
<!--        <view wx:for="123" wx:key="*6">-->
<!--          <text>小哥哥：</text>-->
<!--          <text class="c323232">回复评语回复评语回复评语回复评语回复评语回复评语回复评语回复评语回复评语回复评语回复评语回复评语</text>-->
<!--        </view>-->
<!--      </view>-->
      <text class="f22 mt10" wx:if="{{item.discuss > 0}}">共{{ item.discuss }}条回复</text>
      <view class="f22 mt10 ciBottom">
        <text class="flex1">{{item.create_at}}</text>
        <view class="c787878">
          <view data-url="/commonPage/reply/index?wid={{ item.wid }}&state=2&did={{ item.id }}&type=6" catchtap="goReply" data-index="{{ index }}" data-type="in" class="jwqfont jwqpinglun f28"></view>
          <text catchtap="commentStar" data-index="{{ index }}" class="jwqfont jwqdianzan f28 {{ item.is_star > 0 ? 'cff362c' : '' }}"> {{item.star}}</text>
        </view>
      </view>
    </view>
  </view>
</view>
<!--评论列表 -e-->
<view class="zw100"></view>
<view wx:if="{{ fix }}" class="zw40"></view>
<!--底部评论发表区域 -s-->
<form bindsubmit="sendHundredDiscussSub">
  <view class="detailBottom" style="padding-bottom: {{ fix ? 55:15 }}rpx;">
    <input value="{{commentValue}}" name="comment" focus="{{ focus }}" bindblur="_writeComment" data-type="out" class="dbInput" cursor-spacing="10" placeholder="请输入评论内容"/>
    <button form-type="submit" class="dbSend">发送</button>
    <view class="dbRight">
      <view catchtap="_shareType" class="jwqfont jwqfenxiang">分享</view>
    </view>
  </view>
</form>
<!--底部评论发表区域 -e-->

<!--分享方式选择 -s-->
<view class="mask" wx:if="{{ showShare }}">
<view class="mask-bottom-share animated {{ showShare ? 'slideInUp' : '' }}">
  <view class="bd1 bottom-share">
    <button catchtap="_shareType" open-type="share"><image src="data:image/png;base64,"></image></button>
    <button catchtap="_goPicShare"><image src="data:image/png;base64,"></image></button>
  </view>
  <view catchtap="_shareType" class="tac p200 f34 fw7 cff362c" >取消</view>
</view>
</view>
<!--分享方式选择 -e-->
<!--错误弹窗-->
<toast toast="{{ toast }}"></toast>
